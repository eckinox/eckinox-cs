# editorconfig.org
root = true

[*]
charset = utf-8
end_of_line = LF
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = tab

[*.md]
trim_trailing_whitespace = false

[*.{yml,yaml,php,html,twig,js,mjs,cjs,ts,css,json}]
indent_size = 4

[*.{yml,yaml}]
indent_style = space
