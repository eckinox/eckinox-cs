{
    "name": "eckinox/eckinox-cs",
    "version": "1.2.0",
    "description": "A very opinionated CS/linting setup for PHP projects.",
    "type": "eckinox-metapackage",
    "autoload": {
        "psr-4": {
            "Eckinox\\CodingStandards\\": "src/"
        }
    },
    "extra": {
        "class": "Eckinox\\CodingStandards\\ReplicationHandler"
    },
    "require": {
        "eckinox/installer-plugin": "^1.2.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "friendsoftwig/twigcs": "^5.0",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^1.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-symfony": "^1.1"
    },
    "require-dev": {
        "composer/composer": "^1.3 || ^2.1"
    },
    "license": "MIT",
